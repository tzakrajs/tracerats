<script>
var pfbs = document.getElementsByClassName("pf");
var pfContentList = document.getElementsByClassName("pfContent");
var i;

function profiles() {
    var x;
    var y;
    var idContent = this.classList[1] + "Content";
    var content = document.getElementById(idContent);
    if (this.classList.length == 2) {
        for (x = 0; x < pfbs.length; x++) {
            pfbs[x].classList.remove(pfbs[x].classList[2]);
        }
        for (y = 0; y < pfContentList.length; y++) {
            pfContentList[y].style.display = "none";
        }
        this.classList.toggle(this.classList[1] + "-a");
        content.style.display = "block";
    } else {
        this.classList.toggle(this.classList[1] + "-a");
        content.style.display = "none";
    }
}

for (i = 0; i < pfbs.length; i++) {
    pfbs[i].addEventListener("click", profiles);
}
</script>
