    <div style="margin-left:1rem;">
        <!------- Colourblind Mode CSS ------->
      <style>
      /* The switch - the box around the slider */
      .switch {
          position: relative;
          display: inline-block;
          width: 4rem;
          height: 2.25rem;
      }

      /* Hide default HTML checkbox */
      .switch input {
          opacity: 0;
          width: 0;
          height: 0;
      }

      /* The slider */
      .slider {
          position: absolute;
          cursor: pointer;
          top: 0;
          left: 0;
          right: 0;
          bottom: 0;
          background-color: var(--Bckd);
          -webkit-transition: .4s;
          transition: .4s;
      }

      .slider:before {
          position: absolute;
          content: "";
          height: 1.75rem;
          width: 1.75rem;
          left: 0.25rem;
          bottom: 0.25rem;
          background-color: var(--Light);
          -webkit-transition: .4s;
          transition: .4s;
      }

      input:checked + .slider {
          background-color: var(--Middle);
      }

      input:checked + .slider:before {
          -webkit-transform: translateX(1.75rem);
          -ms-transform: translateX(1.75rem);
          transform: translateX(1.75rem);
      }

      /* Rounded sliders */
      .slider.round {
          border-radius: 2.25rem;
      }

      .slider.round:before {
          border-radius: 50%;
      }
      </style>
    
      <label class="switch" id="cbSlider">
          <input type="checkbox" onclick="cbToggle()">
          <span class="slider round"></span>
      </label>
    </div>
    
  </div>

  <script>
    var cbActive = false;
    var colours = ["sStorm", "dStorm", "sMayhem", "dMayhem", "sSoph", "dSoph", "sAshley", "dAshley", "sMichael", "dMichael", "sDev", "dDev"];
    
    function cbToggle() {
        var r = document.querySelector(':root');
        var rs = getComputedStyle(r);
        if (cbActive == false) {
            for(i = 0; i < colours.length; i++) {
            if(colours[i].substr(0,1) == "s") {
                r.style.setProperty(("--" + colours[i]), rs.getPropertyValue('--Light'));
            } else {
                r.style.setProperty(("--" + colours[i]), rs.getPropertyValue('--Middle'));
            }
            }
            cbActive = true;
        } else {
            for(i = 0; i < colours.length; i++) {
            r.style.setProperty("--" + colours[i], rs.getPropertyValue("--PERM" + colours[i]));
            }
            cbActive = false;
        }
    }
    </script>

</div>

